{
    "name": "To-do/Reminder List",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action":{
        "default_icon": "icon.jpg",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": []
}